{
	"name": "zweefstats",
	"version": "1.4.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"build:server": "node build_server.js",
		"preview:server": "wrangler dev dist/index.mjs",
		"deploy:server": "wrangler publish",
		"proxy:server": "lcp --proxyUrl https://api.zweefstats.nl"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20230221.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.14.0",
		"@tailwindcss/typography": "^0.5.7",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.13",
		"esbuild": "^0.17.10",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"lcp": "^1.1.0",
		"local-cors-proxy": "^1.1.0",
		"postcss": "^8.4.18",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^5.0.3",
		"svelte-preprocess-markdown": "^2.7.3",
		"tailwindcss": "^3.2.1",
		"tslib": "^2.3.1",
		"typescript": "^5.0.2",
		"vite": "^4.2.1",
		"wrangler": "^2.11.1"
	},
	"type": "module",
	"dependencies": {
		"daisyui": "^2.42.1",
		"papaparse": "^5.3.2"
	}
}
